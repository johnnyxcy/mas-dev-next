            "import": "./dist/node.js",
            "require": "./dist/node.cjs"
        }
    },
    "typesVersions": {
        "*": {
            "nls": [
                "./.dist/types/nls.d.ts"
            ],
            "common": [
                "./.dist/types/common.d.ts"
            ],
            "node": [
                "./.dist/types/node.d.ts"
            ]
        }
