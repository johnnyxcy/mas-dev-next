    "type": "module",
    "main": "./.dist/lib/nls.js",
    "types": "./.dist/types/nls.d.ts",
    "files": [
        ".dist"
    ],
    "exports": {
        ".": {
            "import": "./.dist/nls.js",
            "require": "./.dist/nls.cjs"
        },
        "./common": {
            "import": "./dist/common.js",
            "require": "./dist/common.cjs"
        },
