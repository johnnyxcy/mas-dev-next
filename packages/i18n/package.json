{
    "name": "@mas/i18n",
    "version": "1.0.0",
    "description": "",
    "type": "module",
    "files": [
        ".dist"
    ],
    "exports": {
        "./nls": {
            "types": "./.dist/types/src/nls.d.ts",
            "default": "./.dist/lib/nls.js"
        },
        "./common/*": {
            "types": "./.dist/types/src/common/*.d.ts",
            "default": "./.dist/lib/common/*.js"
        },
        "./node/*": {
            "types": "./.dist/types/src/node/*.d.ts",
            "default": "./.dist/lib/node/*.js"
        }
    },
    "scripts": {
        "build": "vite build",
        "test": "vitest --run --coverage",
        "test:ui": "vitest --ui --coverage",
        "lint": "eslint ./src --config ./.eslintrc.cjs"
    },
    "keywords": [
        "Internationalization",
        "Translation",
        "Infrastructure",
        "Maspectra",
        "Maspectra.App"
    ],
    "author": {
        "name": "Maspectra Dev Team",
        "email": "mas@drugchina.net",
        "url": "https://www.maspectra.com"
    },
    "license": "ISC",
    "dependencies": {
        "fs-extra": "latest",
        "inversify": "latest"
    },
    "devDependencies": {
        "@mas/tools": "*",
        "@mas/contribution": "*"
    }
}
